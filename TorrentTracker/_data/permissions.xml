<?xml version="1.0" encoding="utf-8"?>
<permissions>
    <group permission_group_id="torrent" title="صلاحيات التورنت">
        <permission permission_id="view_torrents" type="bool" default_value="1">
            <title>عرض التورنتات</title>
        </permission>
        <permission permission_id="download_torrents" type="bool" default_value="0">
            <title>تحميل التورنتات</title>
        </permission>
        <permission permission_id="upload_torrents" type="bool" default_value="0">
            <title>رفع التورنتات</title>
        </permission>
        <permission permission_id="freeleech_access" type="bool" default_value="0">
            <title>الوصول إلى التورنتات المجانية</title>
        </permission>
    </group>

    <group permission_group_id="vip" title="صلاحيات VIP">
        <permission permission_id="vip_access" type="bool" default_value="0">
            <title>الوصول إلى ميزات VIP</title>
        </permission>
        <permission permission_id="bypass_ratio" type="bool" default_value="0">
            <title>تجاوز نسبة التحميل/الرفع</title>
        </permission>
    </group>
</permissions>
